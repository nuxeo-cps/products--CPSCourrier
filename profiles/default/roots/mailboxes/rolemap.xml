<?xml version="1.0"?>
<!-- Close to what's in workspaces
Supervisor can do what LocalManager can except change perms-->
<rolemap>
  <permissions>
    <permission name="Add portal content" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Contributor"/>
      <role name="Supervisor"/>
    </permission>
    <permission name="Add portal folders" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
    </permission>
    <permission name="Change permissions" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
    </permission>
    <permission name="Change subobjects order" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Contributor"/>
      <role name="Supervisor"/>
    </permission>
    <permission name="Delete objects" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
    </permission>
    <permission name="List folder contents" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
      <role name="Reader"/>
    </permission>
    <permission name="Modify Folder Properties" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
    </permission>
    <permission name="Modify portal content" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
      <role name="Owner"/>
    </permission>
    <permission name="View archived revisions" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
    </permission>
    <permission name="View management screens" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
    </permission>
    <permission name="View" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
      <role name="Reader"/>
    </permission>
    <permission name="WebDAV Lock items" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
      <role name="Owner"/>
    </permission>
    <permission name="WebDAV Unlock items" acquire="False">
      <role name="Manager"/>
      <role name="LocalManager"/>
      <role name="Supervisor"/>
      <role name="Contributor"/>
      <role name="Owner"/>
    </permission>
    <permission name="Manage Subscriptions" acquire="True">
      <role name="Manager"/>
      <role name="LocalManager"/>
    </permission>
  </permissions>
</rolemap>
