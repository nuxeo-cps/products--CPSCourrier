<?xml version="1.0"?>
<object name="local_addressbook" meta_type="CPS Layout">
 <property name="layout_create_method"></property>
 <property name="layout_edit_method"></property>
 <property name="layout_view_method"></property>
 <property name="style_prefix">layout_dir_</property>
 <property name="flexible_widgets"/>
 <property name="validate_values_expr"></property>
 <widget name="email" meta_type="Email Widget">
  <property name="fields">
   <element value="email"/>
  </property>
  <property name="label">label_email</property>
  <property name="label_edit">label_email</property>
  <property name="is_i18n">True</property>
  <property name="display_width">30</property>
  <property name="size_max">0</property>
 </widget>
 <widget name="fullname" meta_type="String Widget">
  <property name="fields">
   <element value="fullname"/>
  </property>
  <property name="label">label_full_name</property>
  <property name="label_edit">label_full_name</property>
  <property name="is_i18n">True</property>
  <property name="hidden_layout_modes">
   <element value="create"/>
   <element value="edit"/>
   <element value="search"/>
  </property>
  <property name="display_width">30</property>
 </widget>
 <widget name="givenName" meta_type="String Widget">
  <property name="fields">
   <element value="givenName"/>
  </property>
  <property name="label">label_first_name</property>
  <property name="label_edit">label_first_name</property>
  <property name="is_i18n">True</property>
 </widget>
 <widget name="local_id" meta_type="Identifier Widget">
  <property name="fields">
   <element value="local_id"/>
  </property>
  <property name="is_required">True</property>
  <property name="label">label_user_name</property>
  <property name="label_edit">label_user_name</property>
  <property name="is_i18n">True</property>
  <property name="readonly_layout_modes">
   <element value="edit"/>
  </property>
 </widget>
 <widget name="mailbox" meta_type="Select Widget">
  <property name="fields">
   <element value="ou"/>
  </property>
  <property name="label">portal_type_Mailbox_title</property>
  <property name="label_edit">portal_type_Mailbox_title</property>
  <property name="is_i18n">True</property>
  <property name="readonly_layout_modes">
   <element value="edit"/>
  </property>
  <property name="translated">False</property>
  <property name="vocabulary">mailboxes_ous</property>
 </widget>
 <widget name="id" meta_type="Directory Multi Id Widget">
 <property name="fields">
  <element value="id"/>
  <element value="local_id"/>
  <element value="ou"/>
 </property>
 <property name="widget_ids">
  <element value="local_id"/>
  <element value="mailbox"/>
 </property>
  <property name="is_required">True</property>
  <!-- No label -->
  <property name="readonly_layout_modes">
   <element value="edit"/>
  </property>
 </widget>
 <widget name="sn" meta_type="String Widget">
  <property name="fields">
   <element value="sn"/>
  </property>
  <property name="label">label_last_name</property>
  <property name="label_edit">label_last_name</property>
  <property name="is_i18n">True</property>
 </widget>
 <table>
  <row>
   <cell name="id" ncols="2"/>
  </row>
  <row>
   <cell name="fullname" ncols="2"/>
  </row>
  <row>
   <cell name="email" ncols="2"/>
  </row>
  <row>
   <cell name="givenName" ncols="2"/>
  </row>
  <row>
   <cell name="sn" ncols="2"/>
  </row>
 </table>
</object>
